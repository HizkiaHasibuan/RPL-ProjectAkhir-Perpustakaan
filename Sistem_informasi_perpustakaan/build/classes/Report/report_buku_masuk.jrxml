		<queryString>
			<![CDATA[SELECT tb_buku.judul,tb_penerbit.nama,tb_buku.tahun_terbit,tb_buku.jumlah,tb_buku.tgl_pendaftaran,tb_rak.no_rak,tb_kota_terbit.nama_kota,tb_tipe_buku.tipe_buku,tb_buku.isbn_issn
FROM tb_buku
INNER JOIN tb_penerbit ON tb_buku.penerbit_id = tb_penerbit.id
INNER JOIN tb_rak ON tb_buku.rak_id = tb_rak.id
INNER JOIN tb_kota_terbit ON tb_buku.kota_terbit_id = tb_kota_terbit.id
INNER JOIN tb_tipe_buku ON tb_buku.tipe_buku_id = tb_tipe_buku.id
WHERE MONTH(tb_buku.tgl_pendaftaran) = $P{month} AND YEAR(tb_buku.tgl_pendaftaran) = $P{year};]]>
